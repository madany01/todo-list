{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"standard"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"warn",
			"tab"
		],
		"no-tabs": "off",
		"no-multiple-empty-lines": [
			"warn",
			{
				"max": 4,
				"maxEOF": 1
			}
		],
		"comma-dangle": "off",
		"max-len": [
			"warn",
			{
				"code": 99
			}
		]
	}
}
